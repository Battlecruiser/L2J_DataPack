	<skill id="22213" levels="1" name="Mysterious Scroll: Drum Melody">
		<!-- "For 60 minutes, receives help from a great spirit to increase Max HP by 20%, rate of critical attack by 20%, rate of Prominent Damage through magic damage by 20%, P. Atk. by 10%, P. Def. by 20%, Atk. Spd. by 20%, M. Atk. by 20%, M. Def. by 20%, Casting Spd. by 20%, and resistance to de-buff by 10%. Decreases Speed by 20%. Bestows the ability to recover as HP 5% of the standard short-range physical damage inflicted on an enemy." -->
		<set name="abnormalLvl" val="4" />
		<set name="abnormalTime" val="3600" />
		<set name="abnormalType" val="MULTI_BUFF" />
		<set name="blockedInOlympiad" val="true" />
		<set name="icon" val="icon.skill1357" />
		<set name="isMagic" val="2" /> <!-- Static Skill -->
		<set name="itemConsumeCount" val="1" />
		<set name="itemConsumeId" val="21200" /> <!-- Mysterious Scroll - Prophecy of Wind -->
		<set name="magicLvl" val="1" />
		<set name="operateType" val="A2" />
		<set name="reuseDelay" val="10000" />
		<set name="targetType" val="SELF" />
		<for>
			<effect name="Buff">
				<mul stat="maxHp" val="1.2" />
				<mul stat="rCrit" val="1.2" />
				<mul stat="cAtk" val="1.2" />
				<mul stat="pAtk" val="1.1" />
				<mul stat="pDef" val="1.2" />
				<mul stat="pAtkSpd" val="1.2" />
				<mul stat="mAtk" val="1.2" />
				<mul stat="mDef" val="1.2" />
				<mul stat="mAtkSpd" val="1.2" />
				<sub stat="debuffVuln" val="10" />
				<mul stat="runSpd" val="0.8" />
				<add stat="absorbDam" val="5" />
			</effect>
		</for>
	</skill>
